DEFINE CLASS _olecalendar AS olecontrol 
 	*< CLASSDATA: Baseclass="olecontrol" Timestamp="" Scale="Pixels" Uniqueid="" Nombre="_olecalendar" Parent="" ObjName="_olecalendar" OLEObject="C:\WINNT\System32\mscomct2.ocx" Value="" />

	*<DefinedPropArrayMethod>
